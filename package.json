{
  "name": "starcast-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@getbrevo/brevo": "^3.0.1",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-email/components": "^0.3.2",
    "@react-pdf/renderer": "^4.3.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@twilio/conversations": "^2.6.2",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "exceljs": "^4.4.0",
    "framer-motion": "^11.15.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "^15.4.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "prisma": "^6.1.0",
    "rate-limiter-flexible": "^7.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "resend": "^4.7.0",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.19.0",
    "twilio": "^5.8.0",
    "typescript": "^5.6.0",
    "zod": "^3.24.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-3000}",
    "start:railway": "node scripts/railway-start.cjs",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "postinstall": "prisma generate",
    "deploy": "node railway-migrate.js",
    "railway:migrate": "node railway-migrate.js",
    "railway:fix-db": "node scripts/fix-railway-database.js",
    "import:excel": "node scripts/import-excel-data.js",
    "test:excel": "node test-excel-import.js",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:setup": "docker-compose up -d postgres && timeout 10 && npx prisma db push && npx prisma db seed",
    "dev:setup": "npm run docker:setup && npm run dev",
    "security:generate": "node scripts/generate-security-config.cjs",
    "security:audit": "npm run type-check && npm run lint:check",
    "railway:debug": "node scripts/railway-debug.cjs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.4.2",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.4.2",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  }
}
